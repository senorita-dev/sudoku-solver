{
    "compilerOptions": {
        "target": "es2021",
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "types": ["cypress", "node"],
        "baseUrl": ".",
        "paths": {
            "@models/*": ["lib/models/*"]
        }
    },
    "include": ["lib", "cypress/e2e"]
}